    "test": "cross-env VITE_TEST=true VITE_API_URL=http://localhost:3000 vitest run",
    "test:e2e": "cross-env VITE_TEST=true playwright test",
    "test:e2e:ui": "cross-env VITE_TEST=true playwright codegen https://app.lms.localhost --viewport-size 1920,1000 & VITE_TEST=true playwright test --ui",
